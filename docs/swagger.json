{
    "swagger": "2.0",
    "info": {
        "description": "URL shortener on go",
        "title": "URL shortener",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/generate": {
            "post": {
                "description": "Принимает оригинальные URL и возвращает их короткие версии",
                "produces": [
                    "application/json"
                ],
                "summary": "Создать короткую ссылку",
                "parameters": [
                    {
                        "description": "Массив оригинальных URL",
                        "name": "original_urls",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "OriginalUrls": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Список оригинальных и сокращенных ссылок",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "links": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Неверный запрос",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Ошибка на сервере",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/get-original": {
            "post": {
                "description": "Принимает сокращённые ссылки и возвращает их оригинальные версии",
                "produces": [
                    "application/json"
                ],
                "summary": "Получить оригинальные ссылки",
                "parameters": [
                    {
                        "description": "Массив сокращённых ссылок",
                        "name": "short_urls",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "ShortUrls": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Список сокращённых и оригинальных ссылок",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "links": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Неверный запрос",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Ошибка на сервере",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}